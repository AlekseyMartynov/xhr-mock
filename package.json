{
  "name": "xhr-mock",
  "version": "1.9.0",
  "description": "Utility for mocking XMLHttpRequests in the browser.",
  "keywords": [
    "mock",
    "xhr",
    "test",
    "fake",
    "request",
    "ajax",
    "browser",
    "xmlhttprequest",
    "jquery",
    "superagent"
  ],
  "repository": "jameslnewell/xhr-mock",
  "main": "lib/index.js",
  "dependencies": {
    "global": "^4.3.0",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "husky": "^0.13.3",
    "jest": "^20.0.3",
    "jest-environment-node-debug": "^2.0.0",
    "lint-staged": "^3.4.2",
    "prettier": "^1.3.1"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./lib --ignore test.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect --debug-brk ./node_modules/.bin/jest --runInBand --env jest-environment-node-debug",
    "precommit": "lint-staged",
    "prepublish": "npm run build && npm run test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --tab-width 2 --single-quote true --trailing-comma none --bracket-spacing false --jsx-bracket-same-line false --semi true",
      "git add"
    ]
  },
  "license": "MIT"
}
